{
  "name": "thedude-map-extractor",
  "version": "1.0.0",
  "description": "Extract and display TheDude network maps from MikroTik router",
  "main": "extract-manager.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/LeeO86/thedude-map-extractor.git"
  },
  "homepage": "https://github.com/LeeO86/thedude-map-extractor",
  "bugs": {
    "url": "https://github.com/LeeO86/thedude-map-extractor/issues"
  },
  "scripts": {
    "start": "node extract-manager.js",
    "server": "node server.js",
    "dev": "nodemon --ignore extracted/ --ignore '*.log' extract-manager.js",
    "dev:server": "nodemon --ignore extracted/ --ignore '*.log' server.js",
    "extract-single": "node extract-single-map.js"
  },
  "dependencies": {
    "dotenv": "^17.0.1",
    "express": "^4.21.0",
    "puppeteer": "^23.10.4",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "nodemonConfig": {
    "ignore": [
      "extracted/",
      "*.log",
      "debug-*.png",
      "*.tmp",
      "*.temp"
    ],
    "watch": [
      "*.js",
      "public/"
    ],
    "ext": "js,json,html,css"
  },
  "keywords": [
    "mikrotik",
    "thedude",
    "network",
    "monitoring",
    "svg",
    "extraction",
    "docker",
    "github-actions"
  ],
  "author": "LeeO86",
  "license": "MIT"
}
